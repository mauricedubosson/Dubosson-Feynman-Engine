def prime_spiral_test():
    x = np.linspace(2, 500, 1000)
    li_x = expi(np.log(x))
    
    correction = np.zeros_like(x)
    for gamma in zeros:
        correction += (2 * np.sqrt(x) / gamma) * np.sin(gamma * np.log(x))
    
    y_true = li_x - correction
    
    plt.figure(figsize=(12, 6))
    plt.plot(x, li_x, '--', color='gray', label="Statistical Average Li(x)")
    plt.plot(x, y_true, color='blue', label="DFE Riemann Staircase (13 Zeros)")
    plt.title("Number Theory: Emergence of the Prime Staircase via Membrane Resonance")
