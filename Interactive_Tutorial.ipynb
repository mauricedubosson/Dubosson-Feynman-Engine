# Generate Data: Newtonian Cooling with a massive shock at t=25s
t = np.linspace(0, 60, 600)
T_amb, T_0, k = 20.0, 100.0, 0.12
T = T_amb + (T_0 - T_amb) * np.exp(-k * t)
T[250:] += 35 * np.exp(-k * (t[250:] - 25)) # The Shock
T_noisy = T + np.random.normal(0, 0.4, 600)

X = torch.tensor(t, dtype=torch.float32).view(-1, 1)
Y = torch.tensor(T_noisy, dtype=torch.float32).view(-1, 1)

# Training
model = DubossonEngine(1, 32, 1)
optimizer = torch.optim.Adam(model.parameters(), lr=0.05)

print("[-] Training DFE v2.0 on Shock Data...")
for _ in range(1500):
    pred = model(X, shock=1.0) 
    loss = torch.mean((pred - Y)**2)
    optimizer.zero_grad(); loss.backward(); optimizer.step()

# Plot Results
plt.figure(figsize=(12, 6))
plt.plot(t, T_noisy, '.', color='gray', alpha=0.3, label="Noisy Data + Shock")
plt.plot(t, model(X).detach().numpy(), color='red', lw=2.5, label="DFE v2.0 Recovery")
plt.axvline(x=25, color='orange', linestyle='--', label="Shock Instant")
plt.title("DFE Resilience: Real-time Recovery from Thermal Perturbation")
